<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm.xsd"
                 version="2.1">

    <!-- QUERY START FILE -->
    <named-query name="att.hql.findByNatureAndRefId">
        <query>
            <![CDATA[from SysAttachment where nature = :nature and ref = :ref]]>
        </query>
    </named-query>
    <named-query name="ftp.hql.queryAllCfg">
        <query>
            <![CDATA[from SysAttachmentConfig]]>
        </query>
    </named-query>
    <!-- QUERY END FILE -->
</entity-mappings>
